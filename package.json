{
  "name": "next-fullstack-starter",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-next": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@heroicons/react": "^2.0.14",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.9.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "4.23.0",
    "@tanstack/react-query-devtools": "^4.23.0",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@types/bcryptjs": "^2.4.2",
    "autoprefixer": "^10.4.13",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "gray-matter": "^4.0.3",
    "lodash-es": "^4.17.21",
    "next": "^13.1.5",
    "next-auth": "^4.19.2",
    "nodemailer": "^6.9.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "superjson": "^1.12.2",
    "tailwindcss": "^3.2.4",
    "zod": "^3.20.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  },
  "license": "MIT"
}
